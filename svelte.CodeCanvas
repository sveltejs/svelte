		"eead44fb-0270-4324-acf1-a27efa869a56": {
			"path": "eead44fb-0270-4324-acf1-a27efa869a56",
			"fileName": "eead44fb-0270-4324-acf1-a27efa869a56",
			"cellName": "Store Subscription - index.js:L78-95",
			"cellId": "eead44fb-0270-4324-acf1-a27efa869a56",
