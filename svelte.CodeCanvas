			"endLine": 108,
			"parentCellId": "b81aec1a-1596-46c3-8878-fb07b9287b1d",
			"parentPath": "packages/svelte/src/compiler/phases/2-analyze/visitors/shared/element.js"
		},
		"521c5e61-52fd-4648-8786-6232a89eb9f3": {
			"path": "521c5e61-52fd-4648-8786-6232a89eb9f3",
			"fileName": "521c5e61-52fd-4648-8786-6232a89eb9f3",
			"cellName": "Flow: Animation - Code Generation - AnimateDirective.js:L11-29",
			"cellId": "521c5e61-52fd-4648-8786-6232a89eb9f3",
			"visible": true,
			"parentCellId": "27907653-90fd-453c-b46f-65e1d450fab1"
		},
		"packages/svelte/src/compiler/phases/3-transform/client/visitors/AnimateDirective.js-simstep-454afc3a-c047-4cee-a583-bc7245205fec": {
			"path": "packages/svelte/src/compiler/phases/3-transform/client/visitors/AnimateDirective.js-simstep-454afc3a-c047-4cee-a583-bc7245205fec",
			"fileName": "AnimateDirective.js",
			"wiki": "The transformer converts the `AnimateDirective` AST node into a call to the Svelte runtime function `$.animation`. This function is responsible for creating and attaching an animation manager to the element at runtime.",
			"cellName": "Flow: Animation - Code Generation - AnimateDirective.js:L11-29",
			"cellId": "521c5e61-52fd-4648-8786-6232a89eb9f3",
			"visible": true,
