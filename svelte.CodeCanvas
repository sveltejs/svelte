			"parentCellId": "a7e1e1b5-09be-49fa-872b-85a01b459532"
		},
		"packages/svelte/src/internal/client/dom/elements/transitions.js-simstep-5d81373e-8cab-4bf7-8e52-0989dd63129b": {
			"path": "packages/svelte/src/internal/client/dom/elements/transitions.js-simstep-5d81373e-8cab-4bf7-8e52-0989dd63129b",
			"fileName": "transitions.js",
			"wiki": "The `AnimationConfig` is passed to the internal `animate` helper, which uses the Web Animations API (`element.animate`) to apply the calculated transform, creating a smooth FLIP (First, Last, Invert, Play) animation for the reordered element.",
			"cellName": "Flow: Animation - Web Animation Execution - transitions.js:L316-461",
			"cellId": "85647cef-0ab8-4bec-b047-ec2147a833db",
			"visible": true,
			"startLine": 316,
			"endLine": 461,
			"parentCellId": "a7e1e1b5-09be-49fa-872b-85a01b459532",
			"parentPath": "packages/svelte/src/internal/client/dom/elements/transitions.js"
